{% extends 'layout.html.twig' %}

{% block content %}
    <form>
        <div>
            <input type="submit" value="Envoyer">
        </div>
    </form>
    {% for post in posts %}
        <article>
            <header>{{ post.pseudo }}</header>
            <footer>{{ post.story }}</footer>
        </article>
    {% endfor %}
{% endblock %}

